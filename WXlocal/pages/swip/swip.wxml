<view class="view-first" style="{{style}}">
  <!--pages/swip/swip.wxml-->
  <text class="texttwo" >欢迎{{userInfo.nickName}}!</text>
  <!-- 简单按钮效果
    <button class="bt1" bindtap="fb">tap</button>
    <button class="bt2" bindtap="fb2">tap2</button>
    <button class="b3t3" bindtap="fb3">tap3</button>
    <button class="bt4" bindtap="fb4">tap4</button>
    <view class="cv1" >红色</view>
    <view class="cv2">绿色</view>
    <view class="cv3">菊色</view>
    <view class="cv2">粉色</view>
  -->
  <button class="backbutton" bindtap = "backTap">返回</button>
  <button class="actbtn" bindtap="actionBtn">ShowAction</button>

  <button class="scan" catchtap="scanBtn">扫描</button>
  <view bindtap="vibrate">动一动</view>
  <input class="phoneCall"  type="text" maxlength="-1" placeholder="输入号码" 
  confirm-type="search" bindfocus="bindFocus" bindinput="bindInput"></input>

  <view id="FZid" bindtap="FZview" data-setDown="1123" data-set-dong="345">复制内容</view>
</view>