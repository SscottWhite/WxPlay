    <view class="cv2">绿色</view>
    <view class="cv3">菊色</view>
    <view class="cv2">粉色</view>
  -->
  <button class="backbutton" bindtap = "backTap">返回</button>
  <button class="actbtn" bindtap="actionBtn">ShowAction</button>

  <button class="scan" catchtap="scanBtn">扫描</button>
  <view bindtap="vibrate">动一动</view>
